{
  "builders": [
    {
      "type": "docker",
      "image": "ubuntu:latest",
      "export_path": "docker-image.tar"
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "script": "scripts/install_dependencies.sh"
    },
    {
      "type": "shell",
      "script": "scripts/build_image.sh"
    }
  ],
  "post-processors": [
    {
      "type": "docker-tag",
      "repository": "adriananogueira/tema_01_final",
      "tag": "latest"
    }
  ]
}
