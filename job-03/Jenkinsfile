pipeline {
    agent any

    stages {

        stage('Pulling from Docker Hub') {
        steps {
            sh 'docker push adriananogueira/tema_01_final:tagname'
          }
        }

        stage('Running Docker image') {
        steps {
            sh 'docker run --name calculator -d --rm -p 8090:8084 adriananogueira/tema-final-01:tagname'
          }
        }
    }
}