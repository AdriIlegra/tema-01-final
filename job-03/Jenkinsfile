pipeline {

   agent any

   stages {
      stage('Download Image from Docker Hub') {
         steps {
            sh 'docker pull adriananogueira/tema_01_final:latest'
         }
      }

      stage('Run Docker Image') {
         steps {
            sh 'docker run --name tema_01_final -p 8084:8084 adriananogueira/tema_01_final:tagname'
            }
      }
   }
}

