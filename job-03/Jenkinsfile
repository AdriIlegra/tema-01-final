pipeline {
    agent any

    stages {
        stage('Pull da Imagem Docker') {
            steps {
                script {
                    // Nome da imagem e tag
                    def dockerImage = 'adriananogueira/tema_01_final'
                    def dockerTag = 'latest'

                    // Fazer pull da imagem Docker
                    sh "docker pull ${dockerImage}:${dockerTag}"
                }
            }
        }

    }
}

